mixin nav(version)
    -var item = version

    if (version == "one")
        nav.one.navbar.navbar-expand-lg.navbar-light
            .container
                button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navbarNavAltMarkup' aria-controls='navbarNavAltMarkup' aria-expanded='false' aria-label='Toggle navigation')
                    span.navbar-toggler-icon
                #navbarNavAltMarkup.collapse.navbar-collapse
                    a.nav-item.text-dark.nav-link(href='#') Features
                    a.nav-item.text-dark.nav-link(href='#') Pricing
                    a.nav-item.text-dark.nav-link.disabled(href='#') Disabled
    
    if (version == "two")
        nav.two.navbar.navbar-expand-lg.navbar-light
            .container
                button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navbarNavAltMarkup' aria-controls='navbarNavAltMarkup' aria-expanded='false' aria-label='Toggle navigation')
                    span.navbar-toggler-icon
                #navbarNavAltMarkup.collapse.navbar-collapse
                    a.nav-item.text-dark.nav-link(href='#') Features
                    a.nav-item.text-dark.nav-link(href='#') Pricing
                    a.nav-item.text-dark.nav-link.disabled(href='#') Disabled
        .menus.container.my-5.py-5
            .row
                .col-md-12
                    ul
                        li Team
                        li Workshops
                        li Blog
                        li contact
    
    if (version == "three")
        nav.three
            .container-fluid.owl-carousel.owl-carousel-indexBottomMenu
                .row.vertical-center.textBigTitle
                    .item   
                        .col-lg-12
                            h3.display-2.p-5(data-bgcolor="#DCC6E0") Contact
                .row.vertical-center.textBigTitle
                    .item   
                        .col-lg-12
                            h3.display-2.p-5(data-bg="image/dummy.jpg") About Me

                .row.vertical-center.textBigTitle
                    .item   
                        .col-lg-12
                            h3.display-2.p-5(data-bgcolor="#FDE3A7") All Project 

                .row.vertical-center.textBigTitle
                    .item   
                        .col-lg-12
                            h3.display-2.p-5(data-bg="image/dummy.jpg") Team

                .row.vertical-center.textBigTitle
                    .item   
                        .col-lg-12
                            h3.display-2.p-5(data-bgcolor="#E0C6C6") Me.
    if (version == "four") 
        nav.four.position-absolute
            .container-fluid
                .row
                    .col-lg-3.menus
                        ul
                            li.display-8 Team
                            li.display-8 Workshops
                            li.display-8 Blog
                            li.display-8 Contact
    if (version == "five")
        nav.five.navbar.navbar-expand-lg.navbar-transparent.navbar-light.pt-3
            .container
                a.navbar-brand.display-5(href='#') andstudio
                button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navbarTogglerDemo02' aria-controls='navbarTogglerDemo02' aria-expanded='false' aria-label='Toggle navigation')
                    span.navbar-toggler-icon
                #navbarSupportedContent.collapse.navbar-collapse
                    ul.navbar-nav.ml-auto.topnav
                        li.nav-item
                            a.display-8.nav-link.px-3(href='single-classic-dark.html') Work
                        li.nav-item
                            a.display-8.nav-link.px-3(href='project-classic-dark.html') Studios
                        li.nav-item
                            a.display-8.nav-link.pl-3(href='blog-classic-dark.html') Contact